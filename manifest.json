{
	"manifest_version": 2,
	"name": "Veganator",
	"version": "1.1",
	"description": "Troque todas as imagens do site por fotos de verduras e legumes.",
	"icons": {
    	"48": "icon48.png",
        "128": "icon128.png"
    },
	"content_scripts": [
	    {
	      "matches": ["*://*/*"],
	      "js": ["jquery.js", "veganator.js"]
	    }
  	]
}